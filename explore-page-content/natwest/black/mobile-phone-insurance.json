                            {
                                "orientation": "column",
                                "content": [
                                    {
                                        "content": [
                                            {
                                                "heading": "Start date",
                                                "content": [
                                                    {
                                                        "type": "marked",
                                                        "content": "Your cover will begin as soon as your account is open.",
                                                        "sequenceNumber": 1
                                                    }
                                                ],
                                                "sequenceNumber": 1
                                            },
                                            {
                                                "heading": "Who is covered?",
                                                "content": [
                                                    {
                                                        "type": "marked",
                                                        "content": "You, your partner (who lives with you in a domestic relationship, whether married or cohabiting) and/or your children under 18 years of age who live at home with you (under 23 if still in full time education and living with you outside of term time) are covered for phones used on a daily basis.\n\nThere is no cover for phones owned by your employer.",
                                                        "sequenceNumber": 1
                                                    }
                                                ],
                                                "sequenceNumber": 2
                                            },
                                            {
                                                "heading": "Enhancements",
                                                "content": [
                                                    {
                                                        "type": "marked",
                                                        "content": "Cosmetic enhancements such as embellishments and plating and phones made wholly or substantially from precious metals are not covered.",
                                                        "sequenceNumber": 1
                                                    }
