                                    {
                                        "heading": "Important",
                                        "content": [
                                            {
                                                "type": "marked",
                                                "content": "This is not a buildings or contents insurance policy or an equipment maintenance contract. It’s recommended you have a building insurance policy covering your home and a contents insurance policy covering your possessions.",
                                                "sequenceNumber": 1
                                            }
                                        ],
                                        "sequenceNumber": 1
                                    }
                                ],
                                "sequenceNumber": 2
                            }
                        ],
                        "sequenceNumber": 2
                    }
